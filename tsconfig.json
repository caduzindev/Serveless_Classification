{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "ES6"
    ],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "types": [
      "reflect-metadata",
      "node",
      "jest"
    ],
    "outDir": "./build-project",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true
  }
}